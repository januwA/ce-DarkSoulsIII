      <AssemblerScript>[ENABLE]
aobscanmodule(hp_INJECT,DarkSoulsIII.exe,89 83 D8 00 00 00 E9 E1)
alloc(newmem,$1000,"DarkSoulsIII.exe"+451065)

// 改为0秒杀
define(killEnemy, 0)

label(myhp)
label(setEnemyHp)
label(setNpcHp)
label(setEnd)

label(return)

{
  if +10 == 0
    我
  else if +24 == 0
    敌人
  else
    npc
}

newmem:
   pushf
   cmp [rbx+10],0
   je myhp
   cmp [rbx+24],0
   je setEnemyHp
   jmp setNpcHp

myhp:
   push rax
   mov rax,[rbx+Dc]
   mov [rbx+D8],rax

   mov rax,[rbx+D8]
   mov [rbx+e4],rax

   mov rax,[rbx+f4]
   mov [rbx+F0],rax
   pop rax
   jmp setEnd

setEnemyHp:
   mov [rbx+D8],killEnemy
   jmp setEnd

// npc改为1，有些npc会变为敌人
setNpcHp:
   mov [rbx+D8],1
   jmp setEnd

setEnd:
   popf
   jmp return

hp_INJECT:
  jmp newmem
  nop
return:
registersymbol(hp_INJECT)

[DISABLE]
hp_INJECT:
  db 89 83 D8 00 00 00
unregistersymbol(hp_INJECT)
dealloc(newmem)

