          <AssemblerScript>{$lua}

--[[
  F12 保存当前定位
  ctrl+F12 读取定位ID，并传送
  所有配置将保存在 pos.ini 文件中
--]]

if syntaxcheck then return end

INI = require "inifile"

local addr = {
  x = "[pPosBase]+80",
  y = "[pPosBase]+88",
  z = "[pPosBase]+84"
}

local iniFIleName = "pos.ini"
io.close(io.open(iniFIleName,'a')) -- 文件不存在则创建



local function _save(newPos)
  local id = inputQuery('保存定位', "给这个定位设置ID",'')
  if id == nil then return end

  local posTable = INI.parse(iniFIleName)
  posTable[id] = newPos
  INI.save(iniFIleName, posTable)
end

-- 从内存中获取当前的位置
local function _getPos()
  return {
   x = readFloat(addr['x']),
   y = readFloat(addr['y']),
   z = readFloat(addr['z'])
  }
end




-- 传送需要获取id
local function _getTpPos()
  local id = inputQuery('获取定位', "输入传送定位ID",'')
  if id == nil then return end

  local posTable = INI.parse(iniFIleName)
  if posTable[id] == nil then
     showMessage(string.format("没有找到[%s]定位", id))
     return
  end

  return posTable[id]
end

-- 传送将坐标写入内存
local function _TP(pos)
  if pos == nil then return end
  writeFloat(addr['x'], pos['x'])
  writeFloat(addr['y'], pos['y'])
  writeFloat(addr['z'], pos['z'])
end




[ENABLE]

-- 保存坐标热键 F1
save_hk = createHotkey(function()

  _save(_getPos())

end, VK_F12)


-- 传送坐标热键 ctrl+F12
read_hk = createHotkey(function()

  _TP(_getTpPos())

end,VK_CONTROL, VK_F12)




[DISABLE]
save_hk.destroy()
read_hk.destroy()


