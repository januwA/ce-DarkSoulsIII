  local base = readInteger("[imyhp]")
  if base == nil then return end

  writeInteger(hpAddr, readInteger(hpAddr)+500)
  writeInteger(mpAddr, readInteger(mpAddr)+100)
  writeInteger(nlAddr, readInteger(nlAddr)+50)
end, 500)
{$asm}

